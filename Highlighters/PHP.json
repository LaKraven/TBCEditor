                                        "Close": " "
                                    }
                                },
                                {
                                    "Type": "Hex",
                                    "Attributes": {
                                        "Element": "Number",
                                        "ParentBackground": true
                                    },
                                    "Delimiters": "*/+-=\\|&()[]{}`~!@,$%^?:;'\".><#'ghijklmnopqrstuvwyzåäö",
                                    "Properties": {
                                        "CloseOnTerm": true,
                                        "CloseOnEol": true
                                    },
                                    "TokenRange": {
                                        "Open": "0"
                                    }
                                }
