                                        "Element": "ReservedWord",
                                        "ParentBackground": true
                                    }
                                },
                                {
                                    "Type": "Symbol",
                                    "Words": "..\r\n:\r\n;\r\n<\r\n>\r\n=\r\n/\r\n*\r\n.\r\n,\r\n-\r\n+\r\n(\r\n)\r\n[\r\n]",
                                    "Attributes": {
                                        "Element": "Symbol",
                                        "ParentBackground": true
                                    }
                                }
                            ],
                            "Set": [
                                {
                                    "Type": "Number",
                                    "Symbols": "0123456789",
                                    "Attributes": {
                                        "Element": "Number",
                                        "ParentBackground": true
                                    }
                                },
                                {
                                    "Type": "Float",
                                    "Symbols": ".0123456789",
                                    "Attributes": {
                                        "Element": "Number",
                                        "ParentBackground": true
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        }
